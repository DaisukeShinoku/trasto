            <table class="table">
              <tr>
                <th><%= f.label :title, 'タイトル' %></th>
                <td><%= f.text_field :title, :size=>"100"%></td>
              </tr>
              <tr>
                <th><%= f.label :visit_date, '訪問日時' %></th>
                <td><%= f.date_field :visit_date %></td>
              </tr>
              <tr>
                <th><%= f.label :story_image, '画像を登録' %></th>
                <td><%= f.file_field :story_image %></td>
              </tr>
              <tr>
                <th><%= f.label :images, 'サブ画像(4枚まで)' %></th>
                <td><%= f.file_field :images, multiple: true %></td>
              </tr>
              <tr>
                <th><%= f.label :content, '本文' %></th>
                <td><%= f.text_area :content, :size=>"100x30"%></td>
              </tr>
              <tr>
                <th></th>
                <td><%= f.submit "#{kind}" , class: "button is-primary"  %></td>
              </tr>
            </table>